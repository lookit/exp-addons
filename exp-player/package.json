{
  "name": "exp-player",
  "version": "0.5.1",
  "description": "The Experimenter player and associated components",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "docs": "yuidoc --lint && yuidoc",
    "build": "ember build",
    "start": "ember server",
    "test": "yarn run check-style && ember test",
    "check-style": "./node_modules/jscs/bin/jscs .",
    "bump-version": "npm --no-git-tag-version version ",
    "preversion": "git-branch-is develop && yarn test",
    "postversion": "yarn run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CenterForOpenScience/exp-addons.git"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "broccoli-sass": "^0.7.0",
    "ember-ajax": "^2.0.1",
    "ember-bootstrap-datetimepicker": "1.0.4",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-moment-shim": "1.3.0",
    "ember-cli-qunit": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-showdown": "2.5.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.8.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-i18n": "4.2.2",
    "ember-load-initializers": "^0.5.1",
    "ember-power-select": "1.0.0-beta.25",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "0.6.0",
    "ember-sinon-qunit": "1.3.4",
    "ember-try": "~0.0.8",
    "ember-welcome-page": "^1.0.1",
    "eonasdan-bootstrap-datetimepicker": "4.15.35",
    "eonasdan-bootstrap-datetimepicker-npm": "4.17.37",
    "jscs": "^3.0.7",
    "loader.js": "^4.0.1",
    "moment": "2.13.0",
    "moment-timezone": "0.5.4",
    "yuidocjs": "^0.10.2"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "broccoli-auto-generated": "0.0.3",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-template-lint": "^0.5.2",
    "ember-cp-validations": "2.9.7",
    "git-branch-is": "^0.1.0"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    }
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "excludeFiles": [
      "package.json",
      "bower.json",
      "testem.js",
      "ember-cli-build.js",
      "config/environment.js",
      "bower_components",
      "node_modules",
      "blueprints",
      "lib",
      "dist",
      "docs",
      "examples",
      "tmp",
      "vendor",
      "app/locales",
      "tests"
    ],
    "requireSpacesInsideObjectBrackets": false,
    "requireSpacesInsideImportedObjectBraces": false,
    "requireSpacesInAnonymousFunctionExpression": false,
    "requireTrailingComma": false,
    "disallowTrailingComma": false,
    "disallowArrayDestructuringReturn": false,
    "disallowQuotedKeysInObjects": false,
    "requirePaddingNewLinesAfterBlocks": false,
    "validateIndentation": 4,
    "requirePaddingNewLinesBeforeLineComments": false,
    "maximumLineLength": false,
    "disallowSpaceBeforeComma": false
  }
}
