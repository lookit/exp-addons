{
  "name": "exp-player",
  "version": "0.5.1",
  "description": "The Experimenter player and associated components",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "docs": "yuidoc --lint && yuidoc",
    "build": "ember build",
    "start": "ember server",
    "test": "yarn run check-style && ember test",
    "check-style": "./node_modules/jscs/bin/jscs .",
    "bump-version": "npm --no-git-tag-version version ",
    "preversion": "git-branch-is develop && yarn test",
    "postversion": "yarn run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CenterForOpenScience/exp-addons.git"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-sass": "^0.7.0",
    "ember-ajax": "^4.0.0",
    "ember-bootstrap-datetimepicker": "1.1.0",
    "ember-cli": "3.5.1",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-jshint": "^2.0.1",
    "ember-cli-moment-shim": "3.7.1",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-showdown": "4.4.4",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "^3.5.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-i18n": "5.3.0",
    "ember-load-initializers": "^2.0.0",
    "ember-power-select": "2.2.0",
    "ember-resolver": "^5.0.1",
    "ember-sinon": "3.0.0",
    "ember-sinon-qunit": "3.3.0",
    "ember-try": "~1.1.0",
    "eonasdan-bootstrap-datetimepicker": "4.17.47",
    "eonasdan-bootstrap-datetimepicker-npm": "4.17.42",
    "jscs": "^3.0.7",
    "loader.js": "^4.7.0",
    "moment": "2.22.2",
    "moment-timezone": "0.5.23",
    "yuidocjs": "^0.10.2"
  },
  "keywords": [
    "ember-addon"
  ],
  "resolutions": {
    "node-sass": "^4.10.0"
  },
  "dependencies": {
    "broccoli-auto-generated": "0.0.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-template-lint": "^0.7.6",
    "ember-cp-validations": "4.0.0-beta.3",
    "git-branch-is": "^2.1.0",
    "typescript": "3.2.1"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    }
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "excludeFiles": [
      "package.json",
      "bower.json",
      "testem.js",
      "ember-cli-build.js",
      "config/environment.js",
      "bower_components",
      "node_modules",
      "blueprints",
      "lib",
      "dist",
      "docs",
      "examples",
      "tmp",
      "vendor",
      "app/locales",
      "tests"
    ],
    "requireSpacesInsideObjectBrackets": false,
    "requireSpacesInsideImportedObjectBraces": false,
    "requireSpacesInAnonymousFunctionExpression": false,
    "requireTrailingComma": false,
    "disallowTrailingComma": false,
    "disallowArrayDestructuringReturn": false,
    "disallowQuotedKeysInObjects": false,
    "requirePaddingNewLinesAfterBlocks": false,
    "validateIndentation": 4,
    "requirePaddingNewLinesBeforeLineComments": false,
    "maximumLineLength": false,
    "disallowSpaceBeforeComma": false
  }
}
